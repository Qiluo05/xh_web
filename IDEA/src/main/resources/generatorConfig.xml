<generatorConfiguration>
    <!-- 数据库连接信息 -->
    <context id="context1" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://124.221.138.245:3306/jsjds_mass"
                        userId="root"
                        password="ac0ea89d49c476a9">
        </jdbcConnection>
        <!-- 实体类、映射文件和Mapper接口的生成目标 -->
